#######################################
# Вычисление чисел Фибоначчи
# Самая нужна программа на самом деле
#######################################

INT n = 10      # Количество чисел Фибоначчи для вычисления
INT fib_0 = 0   # Первое число Фибоначчи
INT fib_1 = 1   # Второе число Фибоначчи
INT fib_next    # Следующее число Фибоначчи

# Печать первых двух чисел Фибоначчи
PRINTL fib_0
PRINTL fib_1

# Цикл для вычисления следующих чисел Фибоначчи
# Счетчик, начинаем с 2, так как уже напечатали 2 числа
INT i = 2
::fibonacci_loop::
IF i < n THEN
  # Вычисление следующего числа Фибоначчи
  fib_next = fib_0 + fib_1
  
  # Печать следующего числа Фибоначчи
  PRINTL fib_next
  
  # Обновление переменных
  fib_0 = fib_1
  fib_1 = fib_next
  
  # Увеличение счетчика
  i = i + 1
    
  # Повторяем цикл
  GOTO fibonacci_loop
ENDIF
